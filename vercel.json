{
    "functions": {
        "api/github-proxy.js": {
            "runtime": "nodejs20.x",
            "maxDuration": 30
        },
        "api/git-proxy.js": {
            "runtime": "nodejs20.x",
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/",
            "destination": "/api/github-proxy.js"
        },
        {
            "source": "/raw/:path*",
            "destination": "/api/github-proxy.js"
        },
        {
            "source": "/repo/:path*",
            "destination": "/api/github-proxy.js"
        },
        {
            "source": "/git/:path*",
            "destination": "/api/git-proxy.js"
        },
        {
            "source": "/download/:path*",
            "destination": "/api/github-proxy.js"
        },
        {
            "source": "/avatar/:path*",
            "destination": "/api/github-proxy.js"
        },
        {
            "source": "/proxy",
            "destination": "/api/github-proxy.js"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET,POST,OPTIONS,HEAD"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "*"
                }
            ]
        }
    ]
}